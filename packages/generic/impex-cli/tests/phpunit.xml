<phpunit
  backupGlobals="false"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
  cacheResult="false"
>
	<testsuites>
		<testsuite name="impex-cli">
			<directory phpVersion="8.0.0" phpVersionOperator=">=" suffix="Test.php">./</directory>
		</testsuite>
	</testsuites>
</phpunit>
